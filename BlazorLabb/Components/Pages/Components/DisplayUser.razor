@using BlazorLabb.Models

<li>
    <p style="width:60px">@UserModel.Id</p>
    <p>@UserModel.Name</p>
    <p style="width:300px">@UserModel.Email</p>
    <p>@UserModel.Address.Street</p>
    <p>@UserModel.Address.City</p>
    <p style="width:150px">@UserModel.Address.ZipCode</p>
    <p>@UserModel.Company.Name</p>
    <p style="width:300px">@UserModel.Company.Catchphrase</p>
</li>

@code {
	[Parameter]
	public User UserModel { get; set; }
}
